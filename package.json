{
  "name": "@nobarrels/root",
  "version": "0.1.0",
  "private": true,
  "description": "",
  "type": "module",
  "module": "index.js",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "clean": "rm -rf ./packages/*/{tsconfig.*tsbuildinfo,dist}",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "prepare": "husky install",
    "test": "cross-env CI=true NODE_ENV=test turbo run test -- --coverage --run",
    "visualise-bundle": "pnpm --filter @nobarrels/frontend visualise-bundle",
    "watch:harbor:dependencies": "turbo run watch --filter=\"!@nobarrels/frontend\" --parallel"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "11.11.4",
    "@emotion/styled": "11.11.5",
    "@cutting/assert": "^0.1.4",
    "@material-table/core": "^6.4.4",
    "@mui/x-date-pickers": "7.9.0",
    "@mui/icons-material": "5.16.0",
    "@mui/material": "5.16.0",
    "@mui/base": "5.0.0-beta.40",
    "@mui/styled-engine": "5.15.14",
    "@mui/styles": "5.16.0",
    "@mui/system": "5.16.0",
    "@mui/types": "7.2.14",
    "@mui/utils": "5.16.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-helmet": "^6.1.0",
    "react-router-dom": "6.24.1",
    "zen-observable": "0.10.0"
