    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "hast-util-to-jsx-runtime": "2.3.0",
    "husky": "^9.0.11",
    "jsdom": "^24.1.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "tsx": "^4.16.2",
    "turbo": "^2.0.6",
