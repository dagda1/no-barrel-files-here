    "pnpm": "9.4.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs,cjs}": [
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "pnpm": {
    "overrides": {
      "eslint": "8.57.0"
    },
    "updateConfig": {
      "ignoreDependencies": [
        "eslint",
        "*eslint*"
      ]
    }
  }
